      \field{title}{Chip and {Skim}: {Cloning} {EMV} {Cards} with the {Pre}-play {Attack}}
      \field{year}{2014}
      \field{pages}{49\bibrangedash 64}
      \range{pages}{16}
      \keyw{EMV,Pre-Play,cloning}
    \endentry
